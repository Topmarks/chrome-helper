{
  "name": "chrome-helper",
  "version": "0.1.1",
  "description": "A helper class for cyrus-and/chrome-remote-interface",
  "main": "lib/chrome-helper.js",
  "scripts": {
    "compile": "babel --presets es2015 --plugins add-module-exports -d lib/ src/",
    "compile-watch": "babel --watch --presets es2015 --plugins add-module-exports -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "mocha --compilers js:babel-core/register test",
    "test-watch": "multi='mocha-osx-reporter=- nyan=-' mocha --compilers js:babel-core/register --watch --reporter mocha-multi test/",
    "coverage": "nyc --require babel-core/register npm test",
    "coveralls": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "help": "topl --help",
    "chrome": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222 --user-data-dir=$TMPDIR/chrome-profiling --no-default-browser-check --bwsi --window-size=\"800,600\" --enable-gpu-benchmarking"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Topmarks/chrome-helper.git"
  },
  "keywords": [
    "topmarks",
    "chrome"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Topmarks/chrome-helper/issues"
  },
  "homepage": "https://github.com/Topmarks/chrome-helper#readme",
  "dependencies": {
    "chrome-remote-interface": "^0.12.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "mocha": "^2.5.3",
    "mocha-multi": "^0.9.0",
    "mocha-osx-reporter": "^0.1.2",
    "nixt": "^0.5.0",
    "nyc": "^6.6.1",
    "sample-plugin": "file:test/fixtures/sample-plugin",
    "topmarks": "^0.3.2"
  }
}
